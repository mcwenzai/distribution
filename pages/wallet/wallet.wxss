page {
  min-height: 100%;
  background: #FBF9FD;
}
.wallet {
  width: 100%;
}
.wallet .withdrawal {
  width: 100%;
  height: 312rpx;
  background: #fff;
  position: relative;
  box-sizing: border-box;
}
.wallet .withdrawal > image {
  width: 710rpx;
  height: 252rpx;
  position: absolute;
  top: 20rpx;
  left: 50%;
  margin-left: -355rpx;
  z-index: 1;
}
.wallet .withdrawal .column {
  width: 710rpx;
  height: 252rpx;
  border-radius: 12rpx;
  z-index: 99;
  position: absolute;
  top: 20rpx;
  left: 50%;
  margin-left: -355rpx;
}
.wallet .withdrawal .column .rule {
  padding-top: 15rpx;
  margin-right: 13rpx;
  padding-right: 7rpx;
}
.wallet .withdrawal .column .rule .description {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.wallet .withdrawal .column .rule .description text {
  font-size: 20rpx;
  font-weight: 500;
  color: #280500;
  margin-right: 7rpx;
  display: block;
  padding: 4rpx 14rpx;
  border-radius: 8rpx;
  background: rgba(255, 255, 255, 0.16);
}
.wallet .withdrawal .column .rule .description image {
  width: 20rpx;
  height: 20rpx;
}
.wallet .withdrawal .column .rule #Rule {
  position: fixed;
  top: 400rpx;
  left: 95rpx;
}
.wallet .withdrawal .column .number {
  display: flex;
  justify-content: space-between;
  align-content: center;
  margin-left: 60rpx;
}
.wallet .withdrawal .column .number .balance {
  display: flex;
  flex-direction: column;
  align-content: center;
  color: #280500;
}
.wallet .withdrawal .column .number .balance .title {
  display: flex;
  align-items: center;
  height: 40rpx;
  line-height: 40rpx;
}
.wallet .withdrawal .column .number .balance .title image {
  width: 22rpx;
  height: 22rpx;
  display: block;
  flex-shrink: 0;
  margin-left: 18rpx;
}
.wallet .withdrawal .column .number .balance text {
  font-size: 24rpx;
  font-weight: 500;
}
.wallet .withdrawal .column .number .balance .money {
  font-size: 48rpx;
  font-weight: bold;
  margin-top: 10rpx;
}
.wallet .withdrawal .column .number .goods {
  margin-top: 50rpx;
  margin-right: 37rpx;
}
.wallet .withdrawal .column .number .goods navigator text {
  font-size: 28rpx;
  font-weight: bold;
  color: #280500;
  margin-right: 12rpx;
}
.wallet .withdrawal .column .number .goods navigator image {
  width: 10rpx;
  height: 18rpx;
}
.wallet .withdrawal .column .received {
  margin-left: 60rpx;
  margin-top: 20rpx;
}
.wallet .withdrawal .column .received text {
  font-size: 28rpx;
  color: #280500;
  font-weight: bold;
}
.wallet .withdrawal .column .received image {
  width: 10rpx;
  height: 18rpx;
  margin-left: 11rpx;
}
.wallet .detailed {
  margin-top: 20rpx;
  width: 100%;
}
.wallet .detailed .order-detailed {
  height: 88rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.wallet .detailed .order-detailed text {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
}
.wallet .detailed .order-detailed .see text {
  font-size: 24rpx;
  font-weight: 500;
  color: #999;
  margin-right: 12rpx;
}
.wallet .detailed .order-detailed .see image {
  width: 10rpx;
  height: 18rpx;
}
.wallet .detailed .detailed-column {
  height: 156rpx;
  background: #fff;
  margin-top: 2rpx;
  padding: 0 20rpx;
}
.wallet .detailed .detailed-column .name {
  padding-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-content: center;
}
.wallet .detailed .detailed-column .name text {
  font-size: 26rpx;
  font-weight: 500;
  color: #333;
}
.wallet .detailed .detailed-column .name .price {
  font-size: 28rpx;
  font-weight: bold;
  color: #F76C3F;
}
.wallet .detailed .detailed-column .cost text {
  font-size: 24rpx;
  font-weight: 500;
  color: #999;
}
